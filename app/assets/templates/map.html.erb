<header></header>

<div class="container col-md-5">
  <h2>Existing Map</h2>

  <ul class="map-list" ng-repeat="marker in markers">
    <li><img src="{{marker.icon.iconUrl}}">&nbsp;&nbsp;{{marker.venue}}<small>&nbsp;&nbsp;"Guba simpy bebo, zecco plazes moola gpokr."</small></li>
  </ul>

  <div class="container-fluid leaflet-map">
    <leaflet defaults="defaults" center="center" markers="markers" event-broadcast="events" height="100vh" width="100%"></leaflet>
  </div>

</div>

